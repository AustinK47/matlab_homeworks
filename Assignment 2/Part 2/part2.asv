message = "this";
a_m = num2str(message)
shift = 2;

a_m = a_m + shift

for i = 1:lenth(a_m)
    if a_m(i) > 172
        a_m(i) = a_m(i) - 97
    end
    if a_m(i) < 97
        a_m(i) = a_m(i) + 172
    end
end

fprintf('%s', a_m)